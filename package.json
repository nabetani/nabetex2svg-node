{
  "name": "nabetex2svg-node",
  "version": "0.0.2",
  "description": "create svg file from tex using mathjax-node module.",
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "tsc": "tsc",
    "sample": "tsc && node build/Release/main.js -t sample/sample.tex -s sample/s0.jsonc -o sample/sample.svg && node build/Release/main.js -t sample/日本語を含む.tex -s sample/s0.jsonc -o sample/日本語を含む.svg"
  },
  "main": "build/Release/main.js",
  "bin": {
    "nabetex2svg": "build/Release/main.js"
  },
  "keywords": [
    "cli",
    "svg",
    "tex",
    "latex"
  ],
  "files": [
    "LICENSE",
    "package-lock.json",
    "sample",
    "build",
    "package.json",
    "src",
    "readme.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nabetani/nabetex2svg-node.git"
  },
  "author": "NABETANi Takenori",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "18.x",
    "@types/yargs": "16.x",
    "typescript": "4.x"
  },
  "dependencies": {
    "jsonc": "2.x",
    "mathjax-node": "2.x",
    "node": "19.x",
    "yargs": "17.x"
  }
}
